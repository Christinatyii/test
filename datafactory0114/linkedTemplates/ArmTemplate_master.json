{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory0114"
		},
		"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://blobstorage2025.blob.core.chinacloudapi.cn/"
		},
		"AzureBlobStoragelinked111_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://blobstorage2025.blob.core.chinacloudapi.cn/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen2.dfs.core.chinacloudapi.cn/"
		},
		"AzureDataLakeStoragelinked222_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakegen2.dfs.core.chinacloudapi.cn/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory0114_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureBlobStoragelinked111_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStoragelinked111_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStoragelinked222_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStoragelinked222_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}